#include<stdio.h>
#include<stdlib.h>
int main()
{
    int i,j,c=0,n,a[100];
    printf("number of elements");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]==a[j])
            {
                c=c+1;
            }
        }
    }
    printf("%d",c);
}
