#include<stdio.h>
int main()
{
    int a[3],b[3],c[2],i,c1=0,c2=0;
    for(i=0;i<3;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<3;i++)
    {
        scanf("%d",&b[i]);
    }
    for(i=0;i<3;i++)
    {
        if(a[i]>b[i])
        {
            c1+=1;
        }
        else if(a[i]<b[i])
        {
            c2+=1;
        }
    }
    c[0]=c1;c[1]=c2;
    for(i=0;i<2;i++)
    {
        printf("%d\t",c[i]);
    }
    return 0;
    
}
