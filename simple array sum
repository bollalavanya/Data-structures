#include<stdio.h>
int main()
{
    int i,n,ar[1000];
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
     scanf("%d",&ar[i]);
    }
    printf("%d",simpleArraySum(ar,n));
}
int simpleArraySum(int *ar,int n)
{
    int i,sum=0;
    for(i=0;i<n;i++)
    {
        sum=sum+ar[i];
    }
    
   return sum; 
}
